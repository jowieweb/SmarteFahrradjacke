<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			 xmlns:app="http://schemas.android.com/apk/res-auto"
			 xmlns:tools="http://schemas.android.com/tools"
			 android:layout_width="match_parent"
			 android:layout_height="match_parent"
			 tools:context="fh.com.smartjacket.fragment.RouteFragment">

	<com.mapquest.mapping.maps.MapView
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/mapquestMapView"
		app:mapquest_app_key="YNF4GZfIelpgaU7ApDMhhDXyMoPYEcT7"/>


	<android.support.design.widget.FloatingActionButton
		android:id="@+id/addRouteActionButton"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="bottom|right"
		android:layout_marginBottom="16dp"
		android:layout_marginRight="16dp"
		android:clickable="true"
		android:focusable="true"
		android:src="@drawable/ic_directions_white_24dp"/>

</FrameLayout>
